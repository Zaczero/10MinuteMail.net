<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>_10MinuteMail.net</RootNamespace>
    <Version>1.1.1</Version>
    <Authors>Zaczero (Kamil Monicz)</Authors>
    <Company></Company>
    <Description>Simple API wrapper for https://10minutemail.net/</Description>
    <Copyright>Copyright Â© Kamil Monicz 2020</Copyright>
    <PackageProjectUrl>https://github.com/Zaczero/10MinuteMail.net</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageTags>10minutemail, 10mail, 10minute, 100minute, disposable, disposable-email, net, standard, email, mail, temporary</PackageTags>
    <PackageId>10MinuteMail.net-API</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Zaczero/10MinuteMail.net</RepositoryUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
  </ItemGroup>

</Project>
